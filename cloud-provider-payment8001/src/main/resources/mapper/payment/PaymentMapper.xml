<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.cloud.payment.dao.PaymentDAO">
    <resultMap id="paymentNap" type="com.example.cloud.payment.entities.PaymentDO">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="serial" column="serial" jdbcType="VARCHAR"/>
    </resultMap>
    <insert id="creat">
        insert into payment (serial)
        values (#{do.serial})
    </insert>
    <select id="getById" resultType="com.example.cloud.payment.entities.PaymentDO">
        select id, serial
        from payment
        where id = #{id}
    </select>

</mapper>